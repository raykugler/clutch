{
  "name": "react-slideshow-image",
  "version": "1.0.1",
  "author": "Femi Oladeji",
  "description": "An image slideshow with react",
  "keywords": [
    "image",
    "react",
    "Image slider",
    "Slideshow",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-foladeji/react-slideshow.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "html-webpack-plugin": "^3.0.0",
    "husky": "^0.14.3",
    "serve": "^6.5.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "npm i --only=dev && webpack && cp -r src/images dist",
    "heroku-postbuild": "npm run build",
    "start": "serve dist/",
    "precommit": "npm run prepublishOnly && git add .",
    "prepublishOnly": "NODE_ENV=production babel src/lib --out-dir lib --copy-files"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "css-loader": "^0.28.10",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "style-loader": "^0.20.2"
  }
}
